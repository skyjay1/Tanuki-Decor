{
  "multipart": [
    {
      "when": { "AND": [ { "side": "left" }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/east" }
    },
    {
      "when": { "AND": [ { "side": "right" }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/west" }
    },

    {
      "when": { "AND": [ { "side": "left" }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/east", "y": 180 }
    },
    {
      "when": { "AND": [ { "side": "right" }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/west", "y": 180 }
    },

    {
      "when": { "AND": [ { "side": "left" }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/east", "y": 90 }
    },
    {
      "when": { "AND": [ { "side": "right" }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/west", "y": 90 }
    },

    {
      "when": { "AND": [ { "side": "left" }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/east", "y": 270 }
    },
    {
      "when": { "AND": [ { "side": "right" }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/wide_streamer/red/west", "y": 270 }
    }
  ]
}