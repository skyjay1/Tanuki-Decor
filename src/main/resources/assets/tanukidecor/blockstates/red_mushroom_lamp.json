{
  "multipart": [
    {
      "when": { "AND": [ { "half": "upper" }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/upper" }
    },
    {
      "when": { "AND": [ { "half": "lower" }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/lower" }
    },
    {
      "when": { "AND": [ { "half": "upper" }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/upper", "y": 180 }
    },
    {
      "when": { "AND": [ { "half": "lower" }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/lower", "y": 180 }
    },
    {
      "when": { "AND": [ { "half": "upper" }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/upper", "y": 90 }
    },
    {
      "when": { "AND": [ { "half": "lower" }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/lower", "y": 90 }
    },
    {
      "when": { "AND": [ { "half": "upper" }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/upper", "y": 270 }
    },
    {
      "when": { "AND": [ { "half": "lower" }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/mushroom_lamp/red/lower", "y": 270 }
    }
  ]
}